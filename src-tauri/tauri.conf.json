{
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "devPath": "http://localhost:1420",
    "distDir": "../dist"
  },
  "package": {
    "productName": "phoenix-auth",
    "version": "0.3.0"
  },
  "tauri": {
    "updater": {
      "active": true,
      "endpoints": [
        "https://gist.githubusercontent.com/liamh101/41911c66c92a74433eea83de69fe45bd/raw/"
      ],
      "windows": {
        "installMode": "passive"
      },
      "dialog": true,
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IEIzRjMxNDIxRTVCNEY0MzAKUldRdzlMVGxJUlR6c3hqOVg5cjVHcHczQThic3laTGVDWlB6aGttUTBqVzVWMWFMTWxkeWJNdk8K"
    },
    "allowlist": {
      "all": false,
      "shell": {
        "all": false,
        "open": true
      },
      "fs": {
        "writeFile": true
      },
      "path": {
        "all": true
      },
      "dialog": {
        "save": true
      }
    },
    "windows": [
      {
        "title": "Phoenix Auth",
        "width": 585,
        "height": 985,
        "resizable": false
      }
    ],
    "security": {
      "csp": null
    },
    "bundle": {
      "active": true,
      "targets": "all",
      "identifier": "phoenix.auth.v1",
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ]
    }
  }
}
